{
  "manifest_version": 2,

  "name": "Balance",
  "description": "Improve your mental health, by assessing and improving your life balance.",
  "version": "1.0",

  "browser_action":
  {
    "default_icon": "resources/icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "js": ["lib/jquery-1.7.2.min.js"],
    }
  ],
  "permissions":
  [
    "activeTab",
    "storage",
  ]
}
